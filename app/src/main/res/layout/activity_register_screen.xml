<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#979797"
    tools:context=".RegisterScreen">

  <ImageView
      android:id="@+id/imageView6"
      android:layout_width="413dp"
      android:layout_height="608dp"
      android:layout_marginTop="4dp"
      android:layout_marginBottom="279dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/vector2" />

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="88dp"
      android:layout_height="86dp"
      android:layout_marginStart="55dp"
      android:layout_marginTop="32dp"
      android:layout_marginEnd="4dp"
      android:layout_marginBottom="490dp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView6"
      app:layout_constraintEnd_toStartOf="@+id/textView3"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toTopOf="@+id/imageView6"
      app:srcCompat="@drawable/trekpal_latest_logo__1__removebg_preview_2" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="147dp"
      android:layout_marginTop="51dp"
      android:layout_marginEnd="84dp"
      android:layout_marginBottom="521dp"
      android:fontFamily="@font/manrope_bold"
      android:text="Create Account"
      android:textColor="#000000"
      android:textSize="24sp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView6"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toTopOf="@+id/imageView6"
      app:layout_constraintVertical_bias="1.0" />

  <EditText
      android:id="@+id/etFullname"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="68dp"
      android:layout_marginTop="57dp"
      android:layout_marginEnd="69dp"
      android:background="@drawable/rectangle_2"
      android:elevation="10dp"
      android:ems="10"
      android:fontFamily="@font/manrope_semibold"
      android:hint="Fullname :"
      android:inputType="text"
      android:padding="15dp"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toBottomOf="@+id/textView3" />

  <EditText
      android:id="@+id/etUsername"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="68dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="69dp"
      android:background="@drawable/rectangle_2"
      android:ems="10"
      android:fontFamily="@font/manrope_semibold"
      android:hint="Username :"
      android:inputType="text"
      android:padding="15dp"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toBottomOf="@+id/etFullname" />

  <EditText
      android:id="@+id/etPassword"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="68dp"
      android:layout_marginTop="9dp"
      android:layout_marginEnd="69dp"
      android:background="@drawable/rectangle_2"
      android:ems="10"
      android:fontFamily="@font/manrope_semibold"
      android:hint="Password :"
      android:inputType="textPassword"
      android:padding="15dp"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toBottomOf="@+id/etUsername" />

  <EditText
      android:id="@+id/etEmail"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="68dp"
      android:layout_marginTop="9dp"
      android:layout_marginEnd="69dp"
      android:background="@drawable/rectangle_2"
      android:ems="10"
      android:fontFamily="@font/manrope_semibold"
      android:hint="Email :"
      android:inputType="textEmailAddress"
      android:padding="15dp"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toBottomOf="@+id/etPassword" />

  <EditText
      android:id="@+id/etPhoneNum"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="68dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="69dp"
      android:background="@drawable/rectangle_2"
      android:ems="10"
      android:fontFamily="@font/manrope_semibold"
      android:hint="Phone Number :"
      android:inputType="phone"
      android:padding="15dp"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toBottomOf="@+id/etEmail" />

  <Spinner
      android:id="@+id/spinnerGender"
      android:layout_width="275dp"
      android:layout_height="52dp"
      android:layout_marginStart="68dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="69dp"
      android:background="@drawable/group_21"
      android:entries="@array/gender_array"
      android:padding="15dp"
      android:spinnerMode="dialog"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toBottomOf="@+id/etPhoneNum" />

  <EditText
      android:id="@+id/etAddress"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="68dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="69dp"
      android:background="@drawable/rectangle_2"
      android:ems="10"
      android:fontFamily="@font/manrope_semibold"
      android:hint="Address :"
      android:inputType="textPostalAddress"
      android:padding="15dp"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/imageView6"
      app:layout_constraintTop_toBottomOf="@+id/spinnerGender" />

  <Button
      android:id="@+id/btnRegister"
      android:layout_width="153dp"
      android:layout_height="67dp"
      android:layout_marginTop="16dp"
      android:backgroundTint="#D3CCFF"
      android:elevation="20dp"
      android:fontFamily="@font/manrope_extrabold"
      android:text="Register"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/etAddress"
      app:layout_constraintStart_toStartOf="@+id/etAddress"
      app:layout_constraintTop_toBottomOf="@+id/etAddress" />

  <Button
      android:id="@+id/btnReturn"
      android:layout_width="117dp"
      android:layout_height="60dp"
      android:layout_marginTop="16dp"
      android:backgroundTint="#9BFFF3"
      android:elevation="20dp"
      android:text="Back"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="@+id/btnRegister"
      app:layout_constraintStart_toStartOf="@+id/btnRegister"
      app:layout_constraintTop_toBottomOf="@+id/btnRegister" />

</androidx.constraintlayout.widget.ConstraintLayout>
